	<!-- X2 -->
	<link name="X2_UL">
    <inertial>
      <origin xyz="-0.707106781 0 -0.707106781" rpy="0 0.785398163 0" />
      <mass value="1" />
      <inertia ixx="1" ixy="0" ixz="0" iyy="0.33" iyz="0" izz="1" />
    </inertial>
    <visual>
      <origin xyz="-0.707106781 0 -0.707106781" rpy="0 0.785398163 0" />
      <geometry>
         <cylinder length="2" radius=".05" />
      </geometry>
      <material name="purple">
	       <color rgba="1 0 1 1" />
      </material>
    </visual>
  </link>

	<link name="X2_UR">
    <inertial>
      <origin xyz="-0.707106781 0 0.707106781" rpy="0 -0.785398163 0" />
      <mass value="1" />
      <inertia ixx="1" ixy="0" ixz="0" iyy="0.33" iyz="0" izz="1" />
    </inertial>
    <visual>
      <origin xyz="-0.707106781 0 0.707106781" rpy="0 -0.785398163 0" />
      <geometry>
         <cylinder length="2" radius=".05" />
      </geometry>
			<material name="purple">
	       <color rgba="1 0 1 1" />
      </material>
    </visual>
  </link>

	 <link name="X2_BL">
    <inertial>
      <origin xyz="0.707106781 0 -0.707106781" rpy="0 -0.785398163 0" />
      <mass value="1" />
      <inertia ixx="1" ixy="0" ixz="0" iyy="0.33" iyz="0" izz="1" />
    </inertial>
    <visual>
      <origin xyz="0.707106781 0 -0.707106781" rpy="0 -0.785398163 0" />
      <geometry>
