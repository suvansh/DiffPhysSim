  </link>



	<!-- JOINTS -->
  <joint name="head" type="fixed">
    <parent link="base_link" />
    <child link="neck" />
    <origin xyz="0 0 0" />
    <axis xyz="0 1 0" />
    <dynamics damping="0.1" />
  </joint>

  <joint name="neck_base" type="continuous">
    <parent link="neck" />
    <child link="arm_left" />
    <origin xyz="0 0 -1" />
    <axis xyz="0 1 0" />
    <dynamics damping="0.1" />
  </joint>

	<!-- X1 -->
	<joint name="shoulder_left" type="continuous">
    <parent link="arm_left" />
    <child link="X1_UL" />
    <origin xyz="1.41421356 0 -1.41421356" />
    <axis xyz="0 1 0" />
    <dynamics damping="0.1" />
  </joint>

	<joint name="X1_UJ" type="continuous">
    <parent link="X1_UL" />
    <child link="X1_UR" />
    <origin xyz="-1.41421356 0 -1.41421356" />
    <axis xyz="0 1 0" />
    <dynamics damping="0.1" />
  </joint>

	<joint name="X1_XJ1" type="fixed">
    <parent link="X1_UL" />
    <child link="X1_BR" />
    <origin xyz="-1.41421356 0 -1.41421356" />
    <axis xyz="0 1 0" />
    <dynamics damping="0.1" />
  </joint>

	<joint name="X1_XJ2" type="fixed">
    <parent link="X1_UR" />
    <child link="X1_BL" />
    <origin xyz="0 0 0" />
    <axis xyz="0 1 0" />
    <dynamics damping="0.1" />
  </joint>

	<joint name="shoulder_right" type="continuous">
    <parent link="X1_UR" />
    <child link="arm_right" />
    <origin xyz="-1.41421356 0 1.41421356" />
    <axis xyz="0 1 0" />
    <dynamics damping="0.1" />
  </joint>

	<joint name="X1_XJ2" type="fixed">
    <parent link="X1_UR" />
    <child link="X1_BL" />
    <origin xyz="0 0 0" />
    <axis xyz="0 1 0" />
    <dynamics damping="0.1" />
  </joint>

	<loop_joint name="neck_loop" type="continuous">
    <axis xyz="0 1 0"/>
    <link1 link="arm_right" rpy="0 0 0" xyz="1.41421356 0 1.41421356"/>
    <link2 link="neck" rpy="0 0 0" xyz="0 0 -1"/>
  </loop_joint>

	<!-- X1 to X2 -->
	<joint name="X1to2L" type="continuous">
    <parent link="X1_BL" />
    <child link="X2_UL" />
    <origin xyz="1.41421356 0 -1.41421356" />
    <axis xyz="0 1 0" />
    <dynamics damping="0.1" />
  </joint>

	<!-- X2 -->

	<joint name="X2_UJ" type="continuous">
    <parent link="X2_UL" />
    <child link="X2_UR" />
    <origin xyz="-1.41421356 0 -1.41421356" />
    <axis xyz="0 1 0" />
    <dynamics damping="0.1" />
  </joint>

	<joint name="X2_XJ1" type="fixed">
    <parent link="X2_UL" />
    <child link="X2_BR" />
    <origin xyz="-1.41421356 0 -1.41421356" />
    <axis xyz="0 1 0" />
    <dynamics damping="0.1" />
  </joint>

	<joint name="X2_XJ2" type="fixed">
    <parent link="X2_UR" />
    <child link="X2_BL" />
    <origin xyz="0 0 0" />
    <axis xyz="0 1 0" />
    <dynamics damping="0.1" />
  </joint>

	<loop_joint name="X1to2R" type="continuous">
    <axis xyz="0 1 0"/>
    <link1 link="X2_UR" rpy="0 0 0" xyz="-1.41421356 0 1.41421356"/>
    <link2 link="X1_BR" rpy="0 0 0" xyz="-1.41421356 0 -1.41421356"/>
  </loop_joint>

</robot>
