<?xml version="1.0"?>

<robot name="DoublePendulum">
  <link name="base_link">
    <visual>
      <geometry>
         <box size=".2 .2 .2" />
      </geometry>
      <material name="green">
	       <color rgba="0 1 0 1" />
      </material>
    </visual>
  </link>

	<link name="neck">
		<inertial>
			<origin xyz="0 0 -0.5" rpy="0 0 0" />
			<mass value="1" />
			<inertia ixx="1" ixy="0" ixz="0" iyy="0.33" iyz="0" izz="1" />
		</inertial>
		<visual>
			<origin xyz="0 0 -0.5" rpy="0 0 0" />
			<geometry>
				 <cylinder length="1" radius=".05" />
			</geometry>
			<material name="red">
				 <color rgba="1 0 0 1" />
			</material>
		</visual>
	</link>

	<link name="arm_left">
    <inertial>
      <origin xyz="0.707106781 0 -0.707106781" rpy="0 -0.785398163 0" />
      <mass value="1" />
      <inertia ixx="1" ixy="0" ixz="0" iyy="0.33" iyz="0" izz="1" />
    </inertial>
    <visual>
      <origin xyz="0.707106781 0 -0.707106781" rpy="0 -0.785398163 0" />
      <geometry>
         <cylinder length="2" radius=".05" />
      </geometry>
      <material name="green">
	       <color rgba="0 1 0 1" />
      </material>
    </visual>
  </link>

	<link name="arm_right">
    <inertial>
      <origin xyz="0.707106781 0 0.707106781" rpy="0 0.785398163 0" />
      <mass value="1" />
      <inertia ixx="1" ixy="0" ixz="0" iyy="0.33" iyz="0" izz="1" />
